{
  "name": "@sync-skeleton/core",
  "version": "0.0.2",
  "description": "Synchronized Skeleton Loader",
  "author": "crutchcorn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/crutchcorn/sync-skeleton.git",
    "directory": "packages/core"
  },
  "homepage": "https://sync-skeleton.dev",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/crutchcorn"
  },
  "scripts": {
    "clean": "premove ./dist ./coverage",
    "test:eslint": "eslint ./src",
    "test:types": "pnpm run \"/^test:types:ts[0-9]{2}$/\"",
    "test:types:ts54": "node ../../node_modules/typescript54/lib/tsc.js",
    "test:types:ts55": "node ../../node_modules/typescript55/lib/tsc.js",
    "test:types:ts56": "node ../../node_modules/typescript56/lib/tsc.js",
    "test:types:ts57": "node ../../node_modules/typescript57/lib/tsc.js",
    "test:types:ts58": "tsc",
    "test:build": "publint --strict",
    "build": "vite build"
  },
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./skeleton.css": "./skeleton.css",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "src",
    "skeleton.css"
  ],
  "dependencies": {
    "@tanstack/store": "^0.7.5"
  }
}
